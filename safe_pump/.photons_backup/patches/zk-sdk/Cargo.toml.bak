[package]
name = "solana-zk-sdk"
description = "Solana ZK SDK (patched for SafePump)"
documentation = "https://docs.rs/solana-zk-sdk"
version = "2.3.13"
authors = [ "SafePump",]
repository = "https://github.com/hamkj7hpo/safe-pump"
homepage = "https://safe-pump.fun"
license = "MIT"
edition = "2021"

[lib]
crate-type = [ "cdylib", "rlib",]

[dependencies]
base64 = "0.22.1"
bytemuck = "1.23.0"
bytemuck_derive = "1.9.3"
merlin = "3.0.0"
num-derive = "0.4"
num-traits = "0.2"
solana-sdk-ids = "2.2.1"
thiserror = "2.0.12"

[dev-dependencies]
solana-keypair = "2.2.1"
tiny-bip39 = "0.8.2"

[lints]

[dependencies.solana-instruction]
version = "2.3.0"
features = [ "std",]

[dependencies.solana-pubkey]
version = "2.4.0"
features = [ "bytemuck",]

[target."cfg(not(target_os = \\\"solana\\\"))".dependencies]
aes-gcm-siv = "0.11.1"
bincode = "1.3.3"
itertools = "0.12.1"
rand = "0.8.5"
serde = "1.0.219"
serde_derive = "1.0.219"
serde_json = "1.0.140"
sha3 = "0.10.8"
solana-derivation-path = "2.2.1"
solana-seed-derivable = "2.2.1"
solana-seed-phrase = "2.2.1"
solana-signature = "2.3.0"
solana-signer = "2.2.1"
subtle = "2.6.1"

[target."cfg(not(target_os = \\\"solana\\\"))".dependencies.curve25519-dalek]
version = "4.1.3"
features = [ "digest", "rand_core", "serde",]

[target."cfg(not(target_os = \\\"solana\\\"))".dependencies.zeroize]
version = "1.7"
features = [ "zeroize_derive",]

[target."cfg(not(target_arch = \\\"wasm32\\\"))".dependencies.solana-pubkey]
version = "2.4.0"
features = [ "bytemuck",]
